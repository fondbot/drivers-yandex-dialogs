{
  "name": "fondbot/drivers-yandex-dialogs",
  "description": "Yandex Dialogs driver for FondBot",
  "type": "library",
  "license": "MIT",
  "authors": [
    {
      "name": "Vladimir Yuldashev",
      "email": "vladimir@fondbot.io"
    }
  ],
  "require": {
    "php": "^7.1",
    "fondbot/framework": "^2.0"
  },
  "require-dev": {
    "phpunit/phpunit": "^7.2"
  },
  "autoload": {
    "psr-4": {
      "FondBot\\Drivers\\YandexDialogs\\": "src/"
    }
  },
  "extra": {
    "branch-alias": {
      "dev-master": "1.0-dev"
    },
    "laravel": {
      "providers": [
        "FondBot\\Drivers\\YandexDialogs\\YandexDialogsServiceProvider"
      ]
    }
  },
  "scripts": {
    "test": "vendor/bin/phpunit --stop-on-failure --no-coverage",
    "cs": "php-cs-fixer fix --allow-risky=yes"
  },
  "config": {
    "preferred-install": "dist",
    "sort-packages": true
  },
  "minimum-stability": "dev",
  "prefer-stable": true
}
